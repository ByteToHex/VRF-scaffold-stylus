{
  "name": "@ss/solidity",
  "version": "1.0.0",
  "description": "TypeScript project for Solidity dApp",
  "keywords": [
    "solidity",
    "foundry",
    "typescript",
    "arbitrum",
    "blockchain"
  ],
  "license": "MIT",
  "author": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean-contracts": "git clean -xdf -e .env -e node_modules/",
    "deploy": "ts-node -r tsconfig-paths/register scripts/deploy_wrapper.ts",
    "dev": "tsc --watch",
    "export-abi": "ts-node -r tsconfig-paths/register scripts/export_abi.ts",
    "info:networks": "ts-node -r tsconfig-paths/register scripts/test_network.ts",
    "lint": "eslint scripts --ext .ts",
    "lint:fix": "eslint scripts --ext .ts --fix",
    "start": "node dist/index.js",
    "test": "forge test"
  },
  "dependencies": {
    "abitype": "^1.0.8",
    "dotenv": "^16.0.0",
    "ethers": "^6.15.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "viem": "^1.19.9"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.3.0",
    "yargs": "^17.7.2"
  },
  "overrides": {
    "chalk": "5.3.0",
    "strip-ansi": "7.1.0",
    "color-convert": "2.0.1",
    "color-name": "1.1.4",
    "is-core-module": "2.13.1",
    "error-ex": "1.3.2",
    "has-ansi": "5.0.1"
  }
}
